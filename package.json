{
  "name": "pockest-helper",
  "private": true,
  "version": "1.2.0-rc.34",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "release:rc": "npm version prerelease --preid=rc",
    "release": "npm version patch",
    "fetch": "node ./scripts/fetchMonsterSheetData.js",
    "gs:upload": "gsutil -m rsync -r -c ./src/data gs://pockest-helper",
    "gs:meta": "gsutil -m setmeta -r -h 'Content-Type:application/json' -h 'Cache-Control:public, max-age=300' gs://pockest-helper",
    "gs": "npm run gs:upload && npm run gs:meta"
  },
  "dependencies": {
    "@crxjs/vite-plugin": "^1.0.14",
    "classnames": "^2.3.2",
    "convert-csv-to-json": "^2.0.0",
    "dotenv": "^16.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "googleapis": "^128.0.0",
    "vite": "^2.9.15"
  }
}
